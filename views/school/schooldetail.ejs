<% var leftnav="school"%>

<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title>留学生管理平台</title>
        <% include ../module/head %>
        <link href="/css/customer.css" rel="stylesheet" type="text/css" />
        <link href="/global/plugins/bootstrap-fileinput/bootstrap-fileinput.css" rel="stylesheet" type="text/css">
        <link href="/global/plugins/bootstrap-modal/css/bootstrap-modal-bs3patch.css" rel="stylesheet" type="text/css">
        <link href="/global/plugins/bootstrap-modal/css/bootstrap-modal.css" rel="stylesheet" type="text/css">

    </head>
    <!-- END HEAD -->

    <body class="page-header-fixed page-sidebar-closed-hide-logo page-content-white">
        <!-- BEGIN HEADER -->
        <% include ../module/top%>
        <!-- END HEADER -->
        <!-- BEGIN HEADER & CONTENT DIVIDER -->
        <div class="clearfix"> </div>
        <!-- END HEADER & CONTENT DIVIDER -->
        <!-- BEGIN CONTAINER -->
        <div class="page-container">
            <!-- BEGIN SIDEBAR -->
            <% include ../module/left%>
            <!-- END SIDEBAR -->
            <!-- BEGIN CONTENT -->
            <div class="page-content-wrapper">
                <!-- BEGIN CONTENT BODY -->
                <div class="page-content">
                    <!-- BEGIN PAGE HEADER-->

                    <!-- BEGIN PAGE BAR -->
                    <div class="page-bar">
                        <ul class="page-breadcrumb">
                          <li>
                              <a href="/school/schoollist">院校管理</a>
                              <i class="fa fa-circle"></i>
                          </li>
                          <li>
                              <a href="/school/schoollist">院校列表</a>
                          </li>
                        </ul>
                    </div>
                    <!-- END PAGE BAR -->
                    <!-- BEGIN PAGE TITLE-->
                    <h3 class="page-title"> 哈佛大学</h3>
                    <!-- END PAGE TITLE-->
                    <!-- END PAGE HEADER-->

                    <div class="profile-content">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="portlet light ">
                                            <div class="portlet-title tabbable-line">
                                                <div class="caption caption-md">
                                                    <i class="icon-globe theme-font hide"></i>
                                                    <span class="caption-subject font-blue-madison bold uppercase"></span>
                                                </div>
                                                <ul class="nav nav-tabs" style="float:left;">
                                                    <li class="active">
                                                        <a href="#tab_1_1" data-toggle="tab">基本信息</a>
                                                    </li>
                                                    <li>
                                                        <a href="#tab_1_3" data-toggle="tab">专业信息</a>
                                                    </li>
                                                </ul>
                                            </div>
                                            <div class="portlet-body">
                                                <div class="tab-content">
                                                    <!-- PERSONAL INFO TAB -->
                                                    <div class="tab-pane active" id="tab_1_1">
                                                        待设计
                                                    </div>
                                                    <!-- END PERSONAL INFO TAB -->
                                                    <!-- CHANGE AVATAR TAB -->
                                                    <!-- END CHANGE AVATAR TAB -->
                                                    <!-- CHANGE PASSWORD TAB -->
                                                    <div class="tab-pane" id="tab_1_3">

                                                      <div class="row">
                                                          <div class="col-md-12">
                                                            <div class="" id="sample_1">
                                                                <table class="table table-striped table-bordered table-hover">
                                                                  <thead class="flip-content">
                                                                      <tr>
                                                                          <th >
                                                                            序号
                                                                          </th>
                                                                          <th>
                                                                            专业
                                                                          </th>
                                                                          <th class="numeric"> 学历 </th>
                                                                          <th class="numeric"> 简介 </th>
                                                                      </tr>
                                                                  </thead>
                                                                  <tbody>
                                                                    <%for(var i =0;i<15;i++){%>
                                                                      <tr>
                                                                          <td style="text-align:center;"> <a href="#"><%= i+1%></a> </td>
                                                                          <td class="numeric"> 法学院--法学 </td>
                                                                          <td class="numeric"> 硕士 </td>
                                                                          <td class="numeric"> 专业排名第五，5年制，毕业后扮发证书 </td>
                                                                      </tr>
                                                                    <%}%>

                                                                  </tbody>
                                                              </table>
                                                            </div>
                                                          </div>
                                                      </div>




                                                    </div>
                                                    <!-- END CHANGE PASSWORD TAB -->
                                                    <!-- PRIVACY SETTINGS TAB -->
                                                    <!-- END PRIVACY SETTINGS TAB -->
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>









                </div>
                <!-- END CONTENT BODY -->
            </div>
            <!-- END CONTENT -->
        </div>

        <!-- responsive -->

        <!-- END CONTAINER -->
        <!-- BEGIN FOOTER -->
        <% include ../module/bottom%>
        <!-- END FOOTER -->
        <% include ../module/foot%>

        <!-- BEGIN PAGE LEVEL PLUGINS -->
        <script src="/global/plugins/bootstrap-modal/js/bootstrap-modalmanager.js" type="text/javascript"></script>
        <script src="/global/plugins/bootstrap-modal/js/bootstrap-modal.js" type="text/javascript"></script>
        <!-- END PAGE LEVEL PLUGINS -->
    </body>

</html>

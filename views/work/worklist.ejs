<% var leftnav="work"%>

<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title>留学生管理平台</title>
        <% include ../module/head %>
        <link href="/css/customer.css" rel="stylesheet" type="text/css" />

    </head>
    <!-- END HEAD -->

    <body class="page-header-fixed page-sidebar-closed-hide-logo page-content-white">
        <!-- BEGIN HEADER -->
        <% include ../module/top%>
        <!-- END HEADER -->
        <!-- BEGIN HEADER & CONTENT DIVIDER -->
        <div class="clearfix"> </div>
        <!-- END HEADER & CONTENT DIVIDER -->
        <!-- BEGIN CONTAINER -->
        <div class="page-container">
            <!-- BEGIN SIDEBAR -->
            <% include ../module/left%>
            <!-- END SIDEBAR -->
            <!-- BEGIN CONTENT -->
            <div class="page-content-wrapper">
                <!-- BEGIN CONTENT BODY -->
                <div class="page-content">
                    <!-- BEGIN PAGE HEADER-->

                    <!-- BEGIN PAGE BAR -->
                    <div class="page-bar">
                        <ul class="page-breadcrumb">
                            <li>
                                <a href="/work/worklist">工单处理</a>
                                <i class="fa fa-circle"></i>
                            </li>
                            <li>
                                <a href="/work/worklist">待处理工单</a>
                            </li>
                        </ul>
                    </div>
                    <!-- END PAGE BAR -->
                    <!-- BEGIN PAGE TITLE-->
                    <h3 class="page-title"> 待处理工单</h3>
                    <!-- END PAGE TITLE-->
                    <!-- END PAGE HEADER-->

                    <%
                    var status = ['材料准备','申请已提交','已退款','完成-未录取','完成-录取'];
                    %>
                      <form class="form-inline" role="form" style="margin-bottom:15px; float:right;">

                        <div class="form-group">
                            <select class="form-control input-small">
                              <option>请选择工单状态</option>
                              <%for(var j=0;j<status.length;j++){%>
                               <option><%= status[j]%></option>
                               <%}%>
                           </select>
                        </div>



                          <div class="input-group input-medium">
                              <input type="text" class="form-control" placeholder="关键字">
                              <span class="input-group-btn">
                                  <button class="btn blue" type="button">查询</button>
                              </span>
                          </div>
                      </form>

                    <div class="row">
                        <div class="col-md-12">
                          <div class="" id="sample_1">
                              <table class="table table-striped table-bordered table-hover">
                                <thead class="flip-content">
                                    <tr>
                                        <th >

                                        </th>

                                        <th> 工单编号 </th>
                                        <th class="numeric"> 客户 </th>
                                        <th class="numeric"> 创建/处理时间  </th>
                                        <th class="numeric"> 处理人 </th>
                                        <th class="numeric">  工单状态</th>
                                        <th class="numeric">  支付状态</th>
                                        <th class="numeric"> 操作 </th>
                                    </tr>
                                </thead>
                                <tbody>
                                  <%for(var i =0;i<15;i++){%>
                                    <tr>
                                        <td> <input type="checkbox" class="checkboxes" value="1" /> </td>
                                        <td> <a href="#">GD10433<%= i%></a> </td>
                                        <td class="numeric"> <a href="/customer/customerdetail">张三</a> </td>
                                        <td class="numeric"> 2017-01-12 12:00 / 2017-02-09 09:22</td>
                                        <td class="numeric"> 小孙</td>
                                        <td class="numeric"> <%= status[i%5]%></td>
                                        <td class="numeric"> 已付订金3000元</td>
                                        <td style="text-align:center;" >
                                          <a href="#" style="margin-right:10px;"><i class="fa fa-link"></i>查看详情</a>
                                          <a href="/customer/create"><i class="fa fa-pencil"></i>编辑</a>
                                        </td>
                                    </tr>
                                  <%}%>

                                </tbody>
                            </table>
                          </div>
                        </div>
                    </div>

                    <div class="row">
                      <div class="col-md-5">
                        </div>
                      <div class="col-md-7">
                        <div class="dataTables_paginate paging_bootstrap_number" id="sample_1_paginate" style="text-align:right;">
                          <ul class="pagination" style="visibility: visible;"><li class="prev disabled"><a href="#" title="Prev"><i class="fa fa-angle-left"></i></a></li><li class="active"><a href="#">1</a></li>
                          <li><a href="#">2</a></li>
                          <li><a href="#">3</a></li>
                          <li><a href="#">4</a></li>
                          <li><a href="#">5</a></li>
                          <li class="next"><a href="#" title="Next"><i class="fa fa-angle-right"></i></a></li></ul>
                        </div>
                      </div>
                    </div>
                </div>
                <!-- END CONTENT BODY -->
            </div>
            <!-- END CONTENT -->
        </div>
        <!-- END CONTAINER -->
        <!-- BEGIN FOOTER -->
        <% include ../module/bottom%>
        <!-- END FOOTER -->
        <% include ../module/foot%>
    </body>

</html>

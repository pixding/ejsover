<div id="create" class="modal bs-modal-lg fade" tabindex="-1" aria-hidden="true" style="display: none;">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                <h4 class="modal-title">选择学校</h4>
            </div>
            <div class="modal-body">

              <div style="min-height:300px" >
                <form class="form-inline" role="form" style="margin-bottom:15px; float:right;">

                  <div class="form-group">
                    <select class="form-control input-small">
                        <option>请选择国家</option>
                        <option>美国</option>
                        <option>日本</option>
                        <option>澳大利亚</option>
                        <option>西班牙</option>
                    </select>
                  </div>
                  <div class="input-group input-medium">
                      <input type="text" class="form-control" placeholder="关键字">
                      <span class="input-group-btn">
                          <button class="btn blue" type="button">查询</button>
                      </span>
                  </div>


                  </form>
                  <div class="dataTables_wrapper" style="margin-top:10px;">

                  <table class="table table-striped table-bordered table-hover table-checkable order-column" id="sample_1">

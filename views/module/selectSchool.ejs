                        <tr>

                            <th style="width:150px;"> 学校 </th>
                            <th > 托福雅思 </th>
                            <th > 学历 </th>
                            <th> 专业 </th>
                            <th > 操作 </th>
                        </tr>
                    </thead>
                    <tbody>
                      <%for(var i =0;i<10;i++){%>
                        <tr>
                            <td> <a target="_blank" href="/school/schooldetail">某某大学00<%= i%></a> </td>
                            <td>6.5分</td>
                            <td class="numeric"> 本科、研究生 </td>
                            <td class="numeric"> 软件工程、会计学、医学、生物工程...等46个专业 </td>
                            <td style="text-align:center;" >
                              <a href="javascript:;" data-dismiss="modal" style="margin-right:10px;">选择</a>
                            </td>
                        </tr>
                      <%}%>

                    </tbody>
                  </table>

                  <div class="row">
                    <div class="col-md-12 col-sm-12">
                      <div class="dataTables_paginate paging_bootstrap_full_number pull-right" id="sample_1_paginate">
                        <ul class="pagination" style="visibility: visible;">
                          <li class="prev disabled"><a href="#" title="First"><i class="fa fa-angle-double-left"></i></a></li>
